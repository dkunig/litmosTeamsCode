<aura:component implements="force:hasRecordID,force:lightningQuickActionWithoutHeader" controller="litmosTeamMembersManual">
    <aura:attribute name="recordId" type="Id"/>
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    <aura:html tag="style">
        .slds-backdrop {
        background: rgba(43, 40, 38, 0) !important;  
        }
        .slds-fade-in-open {
        display:none !important;
       }
        .slds-modal__content {
        	background-color: transparent;
        }
        .slds-modal {
        display: none;
        visibility:hidden !important;
        }
    </aura:html>
</aura:component>